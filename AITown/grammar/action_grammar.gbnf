root ::= Action
ActionType ::= "\"wait\"" | "\"move\"" | "\"attack\"" | "\"pick_up\"" | "'use_item'"
Action ::= "{"   ws   "\"action\":"   ws   ActionType   ","   ws   "\"kwargs\":"   ws   string   "}"
Actionlist ::= "[]" | "["   ws   Action   (","   ws   Action)*   "]"
string ::= "\""   ([^"]*)   "\""
boolean ::= "true" | "false"
ws ::= [ \t\n]*
number ::= [0-9]+   "."?   [0-9]*
stringlist ::= "["   ws   "]" | "["   ws   string   (","   ws   string)*   ws   "]"
numberlist ::= "["   ws   "]" | "["   ws   string   (","   ws   number)*   ws   "]"
